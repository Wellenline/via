{
	"name": "@wellenline/via",
	"version": "2.4.0",
	"description": "Lightweight express like web framework",
	"main": "http.js",
	"author": "Mihkel Baranov",
	"license": "MIT",
	"typings": "http.d.ts",
	"keywords": [
		"http",
		"router",
		"typescript",
		"framework",
		"simple"
	],
	"scripts": {
		"dev": "tsc --watch",
		"build": "tsc",
		"release": "standard-version",
		"test": "ava test/test.js"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.1",
		"@commitlint/config-conventional": "^16.2.1",
		"@types/node": "14.18.2",
		"@types/superagent": "4.1.10",
		"@types/supertest": "^2.0.11",
		"ava": "^4.0.1",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.0.1",
		"husky": "^3.0.5",
		"standard-version": "^9.3.2",
		"supertest": "^3.1.0",
		"tslint": "^5.20.0",
		"typescript": "^3.7.5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wellenline/via.git"
	},
	"bugs": {
		"url": "https://github.com/wellenline/via/issues"
	},
	"homepage": "https://github.com/wellenline/via#readme",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
